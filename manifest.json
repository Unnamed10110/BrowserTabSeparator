{
  "manifest_version": 3,
  "name": "Custom Private New Tab",
  "version": "1.0",
  "description": "Customizes the new private tab page.",
  "permissions": ["tabs", "activeTab"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "incognito": "split",
  "background": {
    "service_worker": "background.js"
  }
}