{
  "manifest_version": 3,
  "name": "Black Background New Tab",
  "version": "1.0",
  "description": "Changes the background color of new tabs to black.",
  "permissions": ["tabs", "activeTab", "storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "host_permissions": ["<all_urls>"],
  "incognito": "spanning"
}
